.openSetting {
  position: fixed;
  width: 100vw;
  height: 100vh;
  box-sizing: border-box;
}

.mark {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.content-wrapper {
  position: absolute;
  top: calc(50% - 150rpx);
  left:calc(50% - 280rpx);
  z-index: 2;
  padding: 0 20rpx;
  height:300rpx;
  width: 560rpx;
  background: #fff;
  font-size: 30rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.title{
  font-size: 30rpx;
  text-align: center;
  border-bottom:1rpx solid #ccc;
  line-height: 80rpx;
}
.content{
  line-height: 60rpx;
}
.btn{
  position: absolute;
  bottom:0;
  left: 0;
  width: 100%;
}